{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    {
      "identifier": "http:default",
      "allow": [{ "url": "http://**" }],
      "deny": []
    },
    "core:default",
    "shell:allow-open",
    "notification:default",
    "http:allow-fetch",
    "http:allow-fetch-read-body",
    "http:allow-fetch-send"
  ]
}